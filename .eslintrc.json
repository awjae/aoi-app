{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["next/core-web-vitals"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "react/jsx-sort-props": [
      2,
      {
        "callbacksLast": true,
        "shorthandFirst": false,
        "shorthandLast": true,
        "ignoreCase": true,
        "noSortAlphabetically": false
      }
    ],
    "sort-keys": [
      "warn",
      "asc",
      { "caseSensitive": true, "natural": false, "minKeys": 2 }
    ]
  }
}
